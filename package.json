    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.2.0",
    "expo": "~47.0.14",
    "expo-dev-client": "~2.0.1",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "firebase": "^9.15.0",
    "nativewind": "^2.0.11",
    "react": "18.1.0",
    "react-native": "0.70.8",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-hr": "^1.1.4",
